{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalEnv": ["GIT_ACCESS_TOKEN", "NEXT_PUBLIC_GOOGLE_ANALYTICS", "SITE_URL"]
}
